<p id="notice"><%= notice %></p>

<h1>Professores</h1>

<table>
  <thead>
  <tr>
    <th><%= @users.human_attribute_name(:ar) %></th>
    <th><%= @users.human_attribute_name(:description) %></th>
    <th><%= @users.human_attribute_name(:email) %></th>
    <th><%= @users.human_attribute_name(:telephone) %></th>
    <th><%= @users.human_attribute_name(:can_time) %></th>
    <th><%= @users.human_attribute_name(:cant_time) %></th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.ar %></td>
      <td><%= user.description %></td>
      <td><%= user.email %></td>
      <td><%= user.telephone %></td>
      <td><%= user.can_time %></td>
      <td><%= user.cant_time %></td>
      <td><%= link_to t('links.edit'), edit_user_registration_path(user) %></td>
      <td><%= link_to t('links.delete'), user_registration_path, method: :delete, data: { confirm: 'VocÃª tem certeza?' } %></td>
    </tr>
  <% end %>
  </tbody>
</table>

